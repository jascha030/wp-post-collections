{
  "name": "jascha030/package-name",
  "description": "Template repo for creating composer projects",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "keywords": [],
  "homepage": "https://github.com/jascha030",
  "autoload": {
    "psr-4": {
        "Jascha030\\Collection\\Post\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Jascha030\\Collection\\Post\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4|^8.0.0|^8.1",
    "illuminate/collections": "^9.34"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.8.0",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "4.4|^5.4|^6.0",
    "roots/wordpress-no-content": "^6.0"
  },
  "scripts": {
    "phpunit": "phpunit -c .phpunit.xml",
    "php-cs-fixer": "php-cs-fixer fix --config .php-cs-fixer.php",
    "test": "@phpunit",
    "format": "@php-cs-fixer"
  },
  "scripts-descriptions": {
    "test": "Runs phpunit with .phpunit.xml",
    "format": "Runs php-cs-fixer with .php-cs-fixer.php."
  }
}
