{
    "name": "jascha030/package-name",
    "description": "Template repo for creating composer projects",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Jascha030",
            "email": "contact@jaschavanaalst.nl"
        }
    ],
    "keywords": [],
    "homepage": "https://github.com/jascha030",
    "autoload": {
        "psr-4": {
            "Jascha030\\Collection\\Post\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jascha030\\Collection\\Post\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.3",
        "illuminate/collections": "^12.49.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "roots/wordpress-no-content": "^6.9.0",
        "symfony/var-dumper": "^7.4",
        "bamarni/composer-bin-plugin": "^1.8"
    },
    "config": {
        "platform-check": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true
        }
    }
}
